{
    "manifest_version": 3,
    "name": "URL Shortener",
    "version": "1.0.0",
    "description": "Quickly shorten your URLs from Chrome",
    "action": {
      "default_popup": "index.html",
      "default_icon": {
        "16": "icon16.png",
        "48": "icon48.png",
        "128": "icon128.png"
      }
    },
    "permissions": ["tabs", "storage"],
    "icons": {
      "16": "icon16.png",
      "48": "icon48.png",
      "128": "icon128.png"
    }
  }
  